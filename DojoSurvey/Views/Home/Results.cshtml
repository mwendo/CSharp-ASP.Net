@model DojoSurvey.Models.DojoStudent

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Results</title>
</head>
<body>
    <div class="container">
        <h1>Submitted Info</h1>
        <h3>Name: @Model.Name</h3>
        <h3>Dojo Location: @Model.Location</h3>
        <h3>Favorite Language: @Model.Language</h3>
        <h3>Comment: @Model.Comment</h3>
    </div>
</body>
</html>